{
  "name": "nishuuu-themes",
  "displayName": "Nishuuu Themes",
  "description": "Beautiful dark themes with Tara color schemes, dynamic accent colors, folder icon sync, and enhanced italic/bold font styling. 24 themes + 6 icon sets with comprehensive syntax highlighting.",
  "version": "1.2.4",
  "icon": "icon.png",
  "publisher": "taraldinn",
  "author": {
    "name": "Nishuuu",
    "email": "your-email@example.com"
  },
  "license": "MIT",
  "galleryBanner": {
    "color": "#0A0A0A",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taraldinn/nishuuu-themes.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark theme",
    "ui",
    "icons",
    "file icons",
    "folder icons",
    "syntax highlighting",
    "accent color",
    "customizable",
    "italic",
    "bold",
    "font styling",
    "tara",
    "nishuuu",
    "carbon",
    "graphene",
    "ocean",
    "teal",
    "palenight",
    "deepforest"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "title": "Nishuuu Themes",
      "properties": {
        "nishuuu.accent": {
          "type": "string",
          "default": "teal",
          "description": "Changes the accent color of the theme",
          "enum": [
            "acid-lime",
            "blue",
            "bright-teal",
            "carbon",
            "cyan",
            "deepforest",
            "graphene",
            "indigo",
            "lime",
            "ocean",
            "orange",
            "palenight",
            "pink",
            "purple",
            "teal",
            "tomato",
            "vira",
            "white",
            "yellow"
          ],
          "enumDescriptions": [
            "Acid Lime - electric lime green",
            "Blue - classic blue accent",
            "Bright Teal - bright cyan-teal",
            "Carbon - elegant gray accent",
            "Cyan - pure cyan blue",
            "Deepforest - natural green accent",
            "Graphene - clean blue accent",
            "Indigo - deep indigo blue",
            "Lime - fresh lime green",
            "Ocean - deep blue accent",
            "Orange - bright orange accent",
            "Palenight - purple accent",
            "Pink - vibrant pink accent",
            "Purple - rich purple accent",
            "Teal - vibrant teal accent",
            "Tomato - vibrant tomato red",
            "Vira - original Vira accent",
            "White - clean white accent",
            "Yellow - golden yellow accent"
          ]
        },
        "nishuuu.customAccent": {
          "type": "string",
          "default": "",
          "description": "Use a custom accent color (hex format like #80CBC4). Overrides the accent setting when provided.",
          "pattern": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
        },
        "nishuuu.outlinedIcons": {
          "type": "boolean",
          "default": false,
          "description": "Use outlined folder icons instead of filled ones"
        },
        "nishuuu.tabIndicator": {
          "type": "string",
          "default": "none",
          "description": "Style of the active tab indicator",
          "enum": [
            "none",
            "fill"
          ],
          "enumDescriptions": [
            "None - no visual indicator (clean look)",
            "Fill - subtle background color for active tab"
          ]
        }
      }
    },
    "commands": [
      {
        "command": "nishuuu.selectAccentColor",
        "title": "Select Accent Color",
        "category": "Nishuuu Themes"
      },
      {
        "command": "nishuuu.resetAccentColor",
        "title": "Reset Accent Color",
        "category": "Nishuuu Themes"
      },
      {
        "command": "nishuuu.toggleOutlinedIcons",
        "title": "Toggle Outlined Icons",
        "category": "Nishuuu Themes"
      }
    ],
    "themes": [
      {
        "label": "Tara Carbon",
        "uiTheme": "vs-dark",
        "path": "./themes/tara-carbon.json"
      },
      {
        "label": "Tara Carbon High Contrast",
